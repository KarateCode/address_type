<% form_for(:address, @address, :url=>{:action=>'update'}) do |f| %>
  <%= f.error_messages %>
  <%= f.hidden_field :address_type, {:value=>@address_type} %>

  <%= render :partial=>'addresses/form', :object=>f %>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Back', {:action=>'index'} %>
